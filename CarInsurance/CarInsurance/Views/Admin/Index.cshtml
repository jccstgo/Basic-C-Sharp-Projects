@using CarInsurance.ViewModels

@model List<InsureesVm>



<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model[0].FirstName)
    </th>
    <th>
      @Html.DisplayNameFor(model => model[0].LastName)
    </th>
    <th>
      @Html.DisplayNameFor(model => model[0].EmailAddress)
    </th>
    <th>
      @Html.DisplayNameFor(model => model[0].Quote)
    </th>
    <th></th>
  </tr>

  @foreach (var insurees in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => insurees.FirstName)
      </td>
      <td>
        @Html.DisplayFor(modelItem => insurees.LastName)
      </td>
      <td>
        @Html.DisplayFor(modelItem => insurees.EmailAddress)
      </td>
      <td>
        @Html.DisplayFor(modelItem => insurees.Quote)
      </td>

    </tr>
  }


</table>


<div>
  @Html.ActionLink("Back to List", "../Insuree/Index")
</div>








